{
  "verbose": true,
  "collectCoverageFrom": ["imports/**/*.{js,jsx}", "!client/index.js"],
  "testPathIgnorePatterns": ["/node_modules/", "/.meteor/"],
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "moduleNameMapper": {
    "^meteor/(.*)": "<rootDir>/meteorMocks.js"
  },
  "coverageDirectory": ".coverage"
}
